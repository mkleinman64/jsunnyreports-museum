<!DOCTYPE html>
<html lang="en">
 
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Martin Kleinman">
 <title>jSunnyreports website: ##settings.website.owner##</title>

    <!-- Bootstrap Core CSS -->
    <link href="bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">
	
    <!-- Custom CSS -->
    <link href="dist/css/sb-admin-2.css" rel="stylesheet">


    <!-- Custom Fonts -->
    <link href="bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div id="wrapper">

		$$menu.htmlinclude$$

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">##records_alltime.title##</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->

			
           $$header.htmlinclude$$
			

            <!-- /.row -->
			<div class="row">
				<div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-table fa-fw"></i> ##records_alltime.days## 
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>##caption.day##</th>
                                            <th>##caption.yield##</th>
                                            <th>##caption.watt##</th>
                                        </tr>
                                    </thead>
                                    <tbody id="bestDaysTableData">
                                    </tbody>
                                </table>
                            </div>						
						</div>
                    </div>
                </div>
				<div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-table fa-fw"></i> ##records_alltime.months## 
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>##caption.month##</th>
                                            <th>##caption.yield##</th>
											<th>##caption.watt##</th>
                                        </tr>
                                    </thead>
                                    <tbody id="bestMonthsTableData">
                                    </tbody>
                                </table>
                            </div>						
						</div>
                    </div>
                </div>				
				<div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-table fa-fw"></i> ##records_alltime.years## 
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>##caption.year##</th>
                                            <th>##caption.yield##</th>
											<th>##caption.watt##</th>
                                        </tr>
                                    </thead>
                                    <tbody id="bestYearsTableData">
                                    </tbody>
                                </table>
                            </div>						
						</div>
                    </div>
                </div>		

			</div>
			
			
            <!-- /.row -->
			$$footer.htmlinclude$$
        <!-- /#page-wrapper -->

		</div>		

        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="bower_components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="bower_components/metisMenu/dist/metisMenu.min.js"></script>
	

    <!-- Custom Theme JavaScript -->
    <script src="dist/js/sb-admin-2.js"></script>
	
	<script src="numeral/numeral.min.js"></script>	


    <!-- JSUNNYREPORTS! -->	
    <script src="customjs/jsr_common.js"></script>
    <script src="customjs/jsr_includes.js"></script>	
    <script src="customjs/jsr_records_alltime.js"></script>
	
</body>

</html>
