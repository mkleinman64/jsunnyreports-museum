<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Martin Kleinman">

    <title>jSunnyreports website: ##settings.website.owner##</title>

    <!-- Bootstrap Core CSS -->
    <link href="bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="dist/css/sb-admin-2.css" rel="stylesheet">


    <!-- Custom Fonts -->
    <link href="bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>
    <div id="wrapper">

		$$menu.htmlinclude$$

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">##day.information.title##</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->

           $$header.htmlinclude$$
			
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-3" id="inverter_row_1">

				</div>
				
                <div class="col-lg-3" id="inverter_row_2">

                </div>

                <div class="col-lg-3" id="inverter_row_3">

                </div>				
				
				
                <!-- /.col-lg-9 -->
                <div class="col-lg-3">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i> ##index.facts##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
								<table class="table table-bordered table-hover table-striped">
								<tbody>
									<tr>
										<td>##caption.today##</td>
										<td class="text-right" id="jsunnyreports_this_dayyield"></td>
										<td>##caption.kwh##</td>
									</tr>												
									<tr>
										<td>##caption.historic##</td>
										<td class="text-right" id="jsunnyreports_historic_dayyield"></td>
										<td>##caption.kwh##</td>
									</tr>
									<tr>
										<td>##caption.thismonth##</td>
										<td class="text-right" id="jsunnyreports_this_monthyield"></td>
										<td>##caption.kwh##</td>
									</tr>
									<tr>
										<td>##caption.lastmonth##</td>
										<td class="text-right" id="jsunnyreports_last_monthyield"></td>
										<td>##caption.kwh##</td>
									</tr>
									<tr>
										<td>##caption.thisyear##</td>
										<td class="text-right" id="jsunnyreports_this_yearyield"></td>
										<td>##caption.kwh##</td>
									</tr>
									<tr>
										<td>##caption.lastyear##</td>
										<td class="text-right" id="jsunnyreports_last_yearyield"></td>
										<td>##caption.kwh##</td>
									</tr>
									<tr>
										<td>##caption.total##</td>
										<td class="text-right" id="jsunnyreports_total_yield"></td>
										<td>##caption.kwh##</td>
									</tr>
									<tr>
										<td>##caption.savings##</td>
										<td class="text-right" id="jsunnyreports_totalsavings"></td>
										<td>##caption.currency##</td>
									</tr>

									
								</tbody>
							</table>

                        </div>
                        <!-- /.panel-body -->
                    </div>
					
					<div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i> ##caption.inverters##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
								<div id="container_pie" style="height: 300px;"></div>						
								<table class="table table-bordered table-hover table-striped">
								<thead>
									<tr>
										<th>##caption.inverter##</th>
										<th class="text-right">##caption.peak##</th>
										<th class="text-right">##caption.now##</th>
										<th class="text-right">##caption.yield##</th>
									</tr>
								</thead>
								<tbody id="inverterTable">
								</tbody>
							</table>

                        </div>
                        <!-- /.panel-body -->
                    </div>						
                    <!-- /.panel -->
					<div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i> ##index.logo##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<a href="http://www.jsunnyreports.com" target="_blank"><img src="jsr_400.jpg" alt="jSunnyreports, monitors the energy revolution!" class="img-responsive"/></a>
                        </div>
                        <!-- /.panel-body -->
                    </div>							
                </div>
                <!-- /.col-lg-3 -->
            </div>
            <!-- /.row -->
			
			$$footer.htmlinclude$$
			
        <!-- /#page-wrapper -->

		</div>		

        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="bower_components/jquery/dist/jquery.min.js"></script>

    <script src="highcharts/highcharts.js"></script>
    <script src="highcharts/highcharts-more.js"></script>
	<script src="highcharts/highcharts-3d.js"></script>	
    <script src="highcharts/modules/exporting.js"></script>	
	
    <!-- Bootstrap Core JavaScript -->
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="bower_components/metisMenu/dist/metisMenu.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="dist/js/sb-admin-2.js"></script>
	
	<script src="numeral/numeral.min.js"></script>	
	
    <!-- JSUNNYREPORTS! -->	
    <script src="customjs/jsr_common.js"></script>
    <script src="customjs/jsr_includes.js"></script>
    <script src="customjs/jsr_dials.js"></script>
	

</body>

</html>
