<!DOCTYPE html>
<html lang="en">

<head>
 
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Martin Kleinman">

 <title>jSunnyreports website: ##settings.website.owner##</title>

    <!-- Bootstrap Core CSS -->
    <link href="bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">
	

    <!-- Custom CSS -->
    <link href="dist/css/sb-admin-2.css" rel="stylesheet">


    <!-- Custom Fonts -->
    <link href="bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div id="wrapper">

		$$menu.htmlinclude$$

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">##month.title##</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->

			
           $$header.htmlinclude$$
			
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-9">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> ##month.chart##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div id="container_chart" style="height: 520px;"></div>
                        </div>
						
                        <div class="panel-body">
                            <div id="container_chart_sum" style="height: 520px;"></div>
                        </div>
						
                        <!-- /.panel-body -->
                    </div>
					
                    <!-- /.panel -->
  
                </div>
                <!-- /.col-lg-9 -->
				<div class="col-lg-3">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-play fa-fw"></i> ##caption.search##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div>
								  <form role="form" id="the-form">
									<div class="form-group">
										<label>##caption.year##</label>
										<div class="input-group">
											<select id="year" name="year" class="form-control">
											</select>
										</div>													
										<label>##caption.month##</label>
										<div class="input-group">
											<select id="month" name="month" class="form-control">
											</select>
										</div>													

									 </div>				
									 <div class="form-group">
										<button type="submit" class="btn btn-default" id="datepickbutton">##caption.select##</button>
									 </div>											 
								  </form>					
							</div>
                        </div>
                        <!-- /.panel-body -->
                    </div>

					<div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-play fa-fw"></i> ##caption.inverters##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div id="container_pie" style="height: 300px;"></div>
							<div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-inverter">
                                    <thead>
                                        <tr>
                                            <th class="text-left">##caption.inverter##</th>
                                            <th class="text-right">##caption.yield##</th>
                                        </tr>
                                    </thead>
                                    <tbody id="inverterdays"> 

                                    </tbody>
                                </table>
                            </div>	
                        </div>
                        <!-- /.panel-body -->
                    </div>										
					
				</div>
 			
            </div>

            <!-- /.row -->
			<div class="row">
				<div class="col-lg-9">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-table fa-fw"></i> ##month.table.title##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-month">
                                    <thead>
                                        <tr>
                                            <th class="text-right">##caption.date##</th>
                                            <th class="text-right">##caption.yield##</th>
											<th class="text-right">##caption.percentage##</th>
                                            <th class="text-right">##caption.peak##</th>
                                            <th class="text-right">##caption.wp##</th>
                                            <th class="text-right">##caption.kwhkwp##</th>
                                            <th class="text-right">##caption.co2saved##</th>
											<th class="text-right">##caption.savings##</th>
                                        </tr>
                                    </thead>
                                    <tbody id="days"> 

                                    </tbody>
                                </table>
                            </div>						
						</div>
						

                    </div>
                </div>
				
			</div>
			
			
            <!-- /.row -->
			$$footer.htmlinclude$$
        <!-- /#page-wrapper -->

		</div>		

        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="bower_components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
	
    <script src="highcharts/highcharts.js"></script>
	<script src="highcharts/highcharts-3d.js"></script>			
    <script src="highcharts/modules/exporting.js"></script>			
	
    <!-- Metis Menu Plugin JavaScript -->
    <script src="bower_components/metisMenu/dist/metisMenu.min.js"></script>
	
    <!-- Custom Theme JavaScript -->
    <script src="dist/js/sb-admin-2.js"></script>

	<script src="numeral/numeral.min.js"></script>

    <!-- JSUNNYREPORTS! -->	
    <script src="customjs/jsr_common.js"></script>
    <script src="customjs/jsr_includes.js"></script>	
    <script src="customjs/jsr_months.js"></script>	
	
	
</body>

</html>
