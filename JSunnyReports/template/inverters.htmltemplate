<!DOCTYPE html>
<html lang="en">

<head> 

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Martin Kleinman">

 <title>jSunnyreports website: ##settings.website.owner##</title>

    <!-- Bootstrap Core CSS -->
    <link href="bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">
	

    <!-- Custom CSS -->
    <link href="dist/css/sb-admin-2.css" rel="stylesheet">


    <!-- Custom Fonts -->
    <link href="bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div id="wrapper">

		$$menu.htmlinclude$$

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">##inverter.title##</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
			
            $$header.htmlinclude$$
			
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-9">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> ##inverter.chart##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                             <div id="container_chart" style="height: 520px;"></div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
  
                </div>
                <!-- /.col-lg-9 -->
				<div class="col-lg-3">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-play fa-fw"></i> ##inverter.select##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div>
								  <form role="form" id="the-form">
									<div class="form-group">
										<label>##caption.inverters##</label>
										<div class="input-group">
											<select id="inverter" name="inverter" class="form-control">
											</select>
										</div>													
									 </div>		
									<div class="form-group">
										<label>##inverter.selectall##</label>
										<div class="input-group">
											<input type="checkbox" id="checkboxAll">
											</input>
										</div>		
									</div>									 
									 <div class="form-group">
										<button type="submit" class="btn btn-default" id="datepickbutton">##caption.select##</button>
									 </div>											 
								  </form>					
							</div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-play fa-fw"></i> ##caption.options##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<div>
								  <form role="form" id="">
									<div class="form-group">
										<label>##inverter.skipfirstyear##</label>
										<div class="input-group">
											<input type="checkbox" id="checkboxFirstYear" name="checkboxFirstYear">
											</input>
										</div>										
									 </div>				
								 
								  </form>					
							</div>
                        </div>
                        <!-- /.panel-body -->
                    </div>					
					
				</div>
 			
            </div>
			
		
            <!-- /.row -->
			$$footer.htmlinclude$$
        <!-- /#page-wrapper -->

		</div>		

        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="bower_components/jquery/dist/jquery.min.js"></script>
	
    <script src="highcharts/highcharts.js"></script>
    <script src="highcharts/modules/exporting.js"></script>		

    <!-- Bootstrap Core JavaScript -->
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="bower_components/metisMenu/dist/metisMenu.min.js"></script>
	
    <!-- Custom Theme JavaScript -->
    <script src="dist/js/sb-admin-2.js"></script>
	
	<script src="numeral/numeral.min.js"></script>	

    <!-- JSUNNYREPORTS! -->	
    <script src="customjs/jsr_common.js"></script>
    <script src="customjs/jsr_includes.js"></script>	
	<script src="customjs/jsr_inverters.js"></script>
	
	
</body>

</html>
