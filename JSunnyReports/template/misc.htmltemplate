<!DOCTYPE html>
<html lang="en">

<head> 

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Martin Kleinman">

 <title>jSunnyreports website: ##settings.website.owner##</title>

    <!-- Bootstrap Core CSS -->
    <link href="bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">


    <!-- Custom CSS -->
    <link href="dist/css/sb-admin-2.css" rel="stylesheet">


    <!-- Custom Fonts -->
    <link href="bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div id="wrapper">

		$$menu.htmlinclude$$

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">##misc.title##</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->

			
          $$header.htmlinclude$$
			
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> ##misc.siteinformation##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
							<table class="table table-bordered table-hover table-striped">
								<tbody id="summary">
								</tbody>
							</table>						
                            
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
  
                </div>
			</div>
			
			<div class="row">
                <!-- /.col-lg-8 -->
                <div class="col-lg-8">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i> ##misc.morefacts##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
								<table class="table table-bordered table-hover table-striped">
								<tbody>
									<tr>
										<td>##caption.sunrise##</td>
										<td class="text-right" id="sunrise"></td>
										<td>&nbsp;</td>
									</tr>
									<tr>
										<td>##caption.sunset##</td>
										<td class="text-right" id="sunset"></td>
										<td>&nbsp;</td>
									</tr>
									<tr>
										<td>##caption.today##</td>
										<td class="text-right" id="today"></td>
										<td>kWh</td>
									</tr>												
									<tr>
										<td>##caption.historic##</td>
										<td class="text-right" id="historic"></td>
										<td>kWh</td>
									</tr>
									<tr>
										<td>##caption.total##</td>
										<td class="text-right" id="total"></td>
										<td>kWh</td>
									</tr>
									<tr>
										<td>##caption.co2saved##</td>
										<td class="text-right" id="co2"></td>
										<td>kg</td>
									</tr>
									<tr>
										<td>##caption.uraniumsaved##</td>
										<td class="text-right" id="uranium"></td>
										<td>g</td>
									</tr>												
								</tbody>
							</table>
                        </div>
                        <!-- /.panel-body -->

                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-4 -->

                <div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i> ##misc.inverterproduction##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
								<table class="table table-bordered table-hover table-striped">
								<thead>
									<tr>
										<th>##caption.inverter##</th>
										<th>##caption.wp##</th>
										<th class='text-right'>##caption.yield##</th>
										<th class='text-right'>##caption.savings##</th>
									</tr>
								</thead>
								<tbody id="totalinverter">
								</tbody>
							</table>
                        </div>
                        <!-- /.panel-body -->

                    </div>
                    <!-- /.panel -->
                </div>			
            <!-- /.row -->				
				
            </div>
			

			
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bell fa-fw"></i> ##misc.historicproduction##
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
								<table class="table table-bordered table-hover table-striped">
								<thead id="historicheader">
								</thead>
								<tbody id="historicbody">
								</tbody>
							</table>
                        </div>
                        <!-- /.panel-body -->

                    </div>
				
				
                </div>
				
                <!-- /.col-lg-8 -->

                <!-- /.col-lg-4 -->
            </div>
            <!-- /.row -->			
			
			$$footer.htmlinclude$$
        <!-- /#page-wrapper -->

		</div>		

        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="bower_components/jquery/dist/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="bower_components/metisMenu/dist/metisMenu.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="dist/js/sb-admin-2.js"></script>
	
	<script src="numeral/numeral.min.js"></script>	
	
    <!-- JSUNNYREPORTS! -->	
    <script src="customjs/jsr_common.js"></script>
    <script src="customjs/jsr_includes.js"></script>	
	<script src="customjs/jsr_misc.js"></script>

</body>

</html>
